{
  "name": "intracs-rnd/inpayment_client",
  "description": "Intracs Payment (Inpayment) client for Laravel",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "IntracsRnd\\InpaymentClient\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.48",
    "phpunit/php-timer": "^6.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpunit/php-invoker": "^4.0",
    "phpunit/phpunit": "^10.5"
  }
}
